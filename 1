<script>
$(function(){
setTabs($('#tabs>li'),$('#play .vod_con'),'active');
var bf_con_1 = $("#bf_con_1 ul li");
var bf_con_1_num = bf_con_1.length;
var x_bf_1 = '';
var sJ,oJ
for(var i=0;i<bf_con_1_num;i++){
if(i % 60==0){
sJ = bf_con_1.eq(i).find("a").attr("title").replace("第","").replace("回","");
sJ = parseFloat(sJ);
if(!sJ){
sJ = (i+1);
}
if(bf_con_1_num-59>i){
oJ = bf_con_1.eq(i+59).find("a").attr("title").replace("第","").replace("回","");
oJ = parseFloat(oJ);
if(!oJ){oJ=(i+60);}
}else{
oJ=bf_con_1.eq(bf_con_1_num-1).find("a").attr("title").replace("第","").replace("回","")
oJ = parseFloat(oJ);
if(!oJ){oJ = (bf_con_1_num);}
}
if( i < 60 )
{
x_bf_1 += '<span class="active">'+sJ+'-'+oJ+'回</span>';
} else{
x_bf_1 += '<span>'+sJ+'-'+oJ+'回</span>';
}
}
}
$("#x_bf_1").html(x_bf_1);
var bf_con_1_str = '';
var ulClass='';
for(var i=0;i<bf_con_1_num;i++){
var aObj = bf_con_1.eq(i).find("a");
var liHtml = '<li class="'+aObj.attr("data-stat-class")+'"><div class="player"><a href="//www.zgpingshu.com/play/'+aObj.attr("data-stat-id")+'/'+aObj.attr("data-stat-mid")+'.html" data-stat-id="'+aObj.attr("data-stat-id")+'" data-stat-mid="'+aObj.attr("data-stat-mid")+'" data-stat-class="'+aObj.attr("data-stat-class")+'" data-stat-title="'+aObj.attr("data-stat-title")+'" target="_blank" title="播放 '+aObj.attr("data-stat-title")+'">'+aObj.attr("data-stat-title")+'</a></div><div class="down"><a href="//www.zgpingshu.com/down/'+aObj.attr("data-stat-id")+'/'+aObj.attr("data-stat-mid")+'.html" target="_blank" title="下载 '+aObj.attr("data-stat-title")+'">下载</a></div></li>';
if(i % 60==0){
if( i < 60 )
{
ulClass = ' class="playlist"';
}else{
ulClass = ' class="playlist hide"';
}
bf_con_1_str += '<ul'+ulClass+'>'+liHtml; 
}else if(i % 60==59){
bf_con_1_str += liHtml+'</ul>';
}else{
bf_con_1_str += liHtml;
}
}
if(bf_con_1_num % 60>0){
bf_con_1_str += '</ul>';
}
$("#bf_con_1").html(bf_con_1_str);
setTabs($('#x_bf_1>span'),$('#bf_con_1>ul.playlist'),'active');
});
